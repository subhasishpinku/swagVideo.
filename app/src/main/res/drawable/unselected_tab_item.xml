<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:left="5dp"
        android:top="5dp"
        android:bottom="5dp">

        <shape
            android:shape="rectangle">

            <corners
                android:radius="10dp">
            </corners>

            <stroke
                android:width="1dp"
                android:color="@android:color/transparent">
            </stroke>
            <solid android:color="@android:color/transparent"/>
        </shape>

    </item>

</layer-list>